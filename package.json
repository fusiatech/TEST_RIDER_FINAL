{
  "name": "fusia-ai",
  "version": "1.0.0",
  "private": true,
  "description": "Fusia AI - adaptive multi-agent engineering workspace",
  "scripts": {
    "dev": "node ./scripts/dev-local.mjs",
    "dev:local": "node ./scripts/dev-local.mjs",
    "dev:clean": "node ./scripts/dev-local.mjs --clean",
    "dev:next": "next dev --turbopack -p 4000",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "trace:validate": "node scripts/trace/validate-trace.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:components": "vitest run --config vitest.component.config.ts",
    "test:components:watch": "vitest --config vitest.component.config.ts",
    "test:components:coverage": "vitest run --config vitest.component.config.ts --coverage",
    "test:all": "npm run test && npm run test:components",
    "test:terminal": "tsx --test tests/terminal-api.integration.test.ts",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:visual": "playwright test e2e/visual-regression.spec.ts",
    "e2e:visual:update": "playwright test e2e/visual-regression.spec.ts --update-snapshots",
    "docker:build": "docker build -t swarm-ui .",
    "docker:run": "docker run -p 3000:3000 --env-file .env swarm-ui",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose --profile dev up",
    "docker:compose:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f swarm-ui",
    "perf:api": "k6 run tests/performance/api-load.js",
    "perf:ws": "k6 run tests/performance/websocket-load.js",
    "perf:stress": "k6 run tests/performance/stress-test.js",
    "perf:k6": "npm run perf:api && npm run perf:ws && npm run perf:stress",
    "perf:api:vitest": "vitest run tests/performance/api-load.test.ts",
    "perf:ws:vitest": "vitest run tests/performance/websocket-load.test.ts",
    "perf:jobs": "vitest run tests/performance/concurrent-jobs.test.ts",
    "perf:vitest": "vitest run tests/performance/*.test.ts",
    "perf:all": "tsx scripts/run-performance-tests.ts",
    "perf:ci": "tsx scripts/run-performance-tests.ts --env=ci --vitest-only",
    "verify:startup": "node ./scripts/startup-smoke.mjs",
    "verify:chat": "node ./scripts/chat-smoke.mjs",
    "audit:brand": "node ./scripts/brand-audit.mjs",
    "audit:action-matrix": "node ./scripts/action-matrix.mjs",
    "audit:logo-integrity": "node ./scripts/logo-integrity.mjs",
    "audit:settings-coverage": "node ./scripts/settings-coverage.mjs",
    "audit:settings-language": "node ./scripts/settings-language.mjs",
    "audit:screen-states": "node ./scripts/screen-states.mjs",
    "verify:ui-smoke": "node ./scripts/ui-smoke.mjs",
    "verify:phase0": "node ./scripts/mode-smoke.mjs",
    "verify:phase1": "node ./scripts/phase1-certify.mjs",
    "verify:agents": "node ./scripts/verify-agents.mjs",
    "verify:providers-runtime": "node ./scripts/verify-providers-runtime.mjs",
    "verify:provider-auth": "tsx ./scripts/diagnose-profile-providers.ts",
    "verify:provider-auth-live": "node ./scripts/verify-provider-auth.mjs",
    "verify:profile-runs": "tsx ./scripts/verify-profile-runs.ts",
    "verify:fast-live": "node ./scripts/verify-fast-live.mjs",
    "verify:run-matrix": "node ./scripts/verify-run-matrix.mjs",
    "verify:provider-failover": "node ./scripts/verify-provider-failover.mjs",
    "verify:phase-agent-runtime": "npm run verify:provider-auth-live && npm run verify:run-matrix && npm run verify:provider-failover",
    "verify:ws-run": "node ./scripts/ws-run-smoke.mjs",
    "verify:v7": "tsx ./scripts/execute-v7.ts",
    "test:contract": "vitest run tests/contract",
    "test:contract:verify": "vitest run tests/contract/provider.verify.test.ts",
    "report:api-inventory": "node ./scripts/report-api-inventory.mjs",
    "openapi:sync-missing": "node ./scripts/sync-openapi-missing.mjs",
    "migrate:integrations": "node ./scripts/migrate-global-integrations-to-user-scope.mjs",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.57.2",
    "@opentelemetry/semantic-conventions": "^1.40.0",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-virtual": "^3.13.19",
    "@types/react-speech-recognition": "^3.9.6",
    "ansi-to-html": "^0.7.2",
    "bufferutil": "^4.1.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dictionary-en": "^4.0.0",
    "framer-motion": "^12.34.3",
    "js-yaml": "^4.1.0",
    "lowdb": "^7.0.1",
    "lucide-react": "^0.468.0",
    "mermaid": "^11.12.3",
    "monaco-languageclient": "^8.1.1",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "node-pty": "^1.0.0",
    "nspell": "^2.1.5",
    "prom-client": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-speech-recognition": "^4.0.1",
    "recharts": "^3.7.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^1.24.0",
    "sonner": "^1.7.1",
    "swagger-ui-react": "^5.17.0",
    "tailwind-merge": "^2.6.0",
    "utf-8-validate": "^6.0.6",
    "vscode-languageclient": "^9.0.1",
    "vscode-ws-jsonrpc": "^3.3.2",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@lhci/cli": "^0.14.0",
    "@pact-foundation/pact": "^13.2.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.19.13",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.5.13",
    "@vitest/coverage-v8": "^3.2.4",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  }
}

